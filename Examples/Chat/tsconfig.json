{
   "compilerOptions": {
        "removeComments": true,
        "declaration": true,
        "module": "commonjs",
        "target": "es2015",
        "sourceMap": true,
        "watch": false,
        "allowUnreachableCode": false,
        "allowUnusedLabels": false,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitUseStrict": false,
        "noFallthroughCasesInSwitch": true,
        "types": [
            "node"
        ]
    }
}