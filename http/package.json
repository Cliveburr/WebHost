{
  "name": "webhost",
  "version": "0.1.2",
  "description": "A NodeJS framework for create general purpose web services.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build-clean && tsc --watch",
