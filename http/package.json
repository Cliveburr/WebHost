    "pack-clean": "if exist \"dist\" (rmdir /s /q dist)"
  },
  "keywords": [
    "webhost",
    "web",
    "host",
    "framework"
  ],
  "author": "Adilson Manzoli Junior",
  "repository": "Cliveburr/WebHost",
  "license": "ISC",
  "dependencies": {
    "providerjs": "^0.0.15"
  },
  "devDependencies": {
    "@types/node": "^18.11.3",
    "typescript": "^4.8.4"
  }
}
